#include <stdio.h>

int main() {
    float a1, a2, a3;
    float b1, b2, b3;
    float c1, c2, c3;
    float r1, r2, r3;
    
    scanf("%f %f %f", &a1, &a2, &a3);
    scanf("%f %f %f", &b1, &b2, &b3);
    scanf("%f %f %f", &c1, &c2, &c3);
    
    r1 = (a1 * 2 + a2 * 3 + a3 * 5) / 10;
    r2 = (b1 * 2 + b2 * 3 + b3 * 5) / 10;
    r3 = (c1 * 2 + c2 * 3 + c3 * 5) / 10;
    
    printf("%.1f\n", r1);
    printf("%.1f\n", r2);
    printf("%.1f\n", r3);

    return 0;
}
